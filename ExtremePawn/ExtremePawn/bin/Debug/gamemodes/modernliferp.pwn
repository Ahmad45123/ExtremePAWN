// Valid component iterator
stock ValidComponents@YSII_Ag(modelid, start) {
        static cur;
        static modelidx;

        if (start == -1) {
                if (modelid < 400 || modelid > 611)
                        return -1;

                modelidx = modelid - 400;
                cur = 0;
        }

        if (g_LegalMods[modelidx]{cur})
                return 1000 + g_LegalMods[modelidx]{cur++};

        return -1;
}

