    SendClientMessage(playerid, COLOR_ORANGE, string);
    format(string, sizeof(string), "{FF9900}Firearms License: {FFFFFF}%s", text2);
    SendClientMessage(playerid, COLOR_ORANGE, string);
    format(string, sizeof(string), "{FF9900}Pilot License: {FFFFFF}%s", text3);
