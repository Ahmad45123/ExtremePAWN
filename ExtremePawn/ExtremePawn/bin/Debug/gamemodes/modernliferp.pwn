        if(!SMSes[playerid])
        {
            SMSes[playerid] = 1;
            SendClientMessage(playerid, COLOR_LIGHTBLUE, " You have started listening to SMSes.");
        }
        else
        {
            SMSes[playerid] = 0;
            SendClientMessage(playerid, COLOR_LIGHTBLUE, " You have stopped listening to SMSes.");
        }
    }
    else if(!strcmp(params, "pm", true))
    {
        if(!PMs[playerid])
        {
