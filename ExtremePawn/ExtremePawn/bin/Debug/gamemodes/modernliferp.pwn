    format(string, sizeof(string), "{FF0000}[Admin Warn]{FF6347} %s has given %s the %s faction leadership.", NORPN(playerid), NORPN(playerb), RPFNEx(playerb));
    Log("logs/factions.log", string);
    return 1;
