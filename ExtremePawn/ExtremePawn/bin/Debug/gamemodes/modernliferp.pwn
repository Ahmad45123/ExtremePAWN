    if(PlayerInfo[playerid][pJob] == JOB_LAWYER || PlayerInfo[playerid][pVIPJob] == JOB_LAWYER) SendClientMessage(playerid, COLOR_YELLOW, "LAWYER: {FFFFFF}/free /defend /wanted");
