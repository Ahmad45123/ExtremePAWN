    if(PlayerInfo[playerb][pWanted] >= 5){format(string, sizeof(string), "{FF9900}Crime 5: {FFFFFF}%s", PlayerInfo[playerb][pWanted5]); SendClientMessage(playerid, COLOR_ORANGE, string);}
