        new idx = 1;
        while(idx < MAX_FAMILIES)
        {
            fread(file, string);
            split(string, finfo, '|');
            FamInfo[idx][fTaken] = strval(finfo[0]);
            format(FamInfo[idx][fName], 32, "%s", finfo[1]);
