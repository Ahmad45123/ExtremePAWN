    for(new idx=1; idx<MAX_GATES; idx++)
    {
        if(GateInfo[idx][gModel] && IsPlayerInRangeOfPoint(playerid, 15, GateInfo[idx][gCX], GateInfo[idx][gCY], GateInfo[idx][gCZ]))
