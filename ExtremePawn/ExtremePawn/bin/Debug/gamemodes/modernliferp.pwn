                    if(IsAHSF(i)) copsonline++;
                    if(IsAFBI(i)) copsonline++;
                }
                foreach(Player, i)
                {
                    if(IsACop(i))
                    {
                        new Saved = (TotalRobbers*50000-(TotalStolen*50000));
