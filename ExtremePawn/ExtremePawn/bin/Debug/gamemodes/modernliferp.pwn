            SendClientMessage(playerid, COLOR_LIGHTBLUE, " You have started listening to PMs.");
        }
        else
        {
            PMs[playerid] = 0;
            SendClientMessage(playerid, COLOR_LIGHTBLUE, " You have stopped listening to PMs.");
        }
    }
