    else if(!strcmp(params, "packages", true, 8))
    {
        new confirm[8], veh;
        if(sscanf(params, "s[32]s[8]", params, confirm)) return SendClientMessage(playerid, COLOR_WHITE, "USAGE: /drop packages confirm");
        if(!strcmp(confirm, "confirm", true))
        {
