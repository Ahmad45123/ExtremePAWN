    format(string, sizeof(string), " You have deposited $%d into your bank account, your bank balance is now: $%d.", amount, PlayerInfo[playerid][pBank]);
    SendClientMessage(playerid, COLOR_LIGHTBLUE, string);
    return 1;
}

